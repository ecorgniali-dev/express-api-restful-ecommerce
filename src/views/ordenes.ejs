<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <ul>
        <% if (ordenes.length) { %>
            <% ordenes.forEach(e => { %>
                <li><%=e.timestamp %> - <%=e.estado %> - <%=e.email %> - <%=e.direccion %> </li>
                <ul>
                    <% e.productos.forEach(element => { %>
                        <li><%=element.item %> - <%=element.cantidad %></li>
                    <% }) %>
                </ul>
            <% }) %>
        <% } %>
    </ul>

</body>

</html>